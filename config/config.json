{
  "data": {
    "file_path": "data/raw/Feature_Extraction_156_best_math_transformed_SignSafe.xlsx",
    "target_sin": "target_wave_direction_deg_sin",
    "target_cos": "target_wave_direction_deg_cos",
    "hs_column": "sea_elevation_significant_height_Hs_m",
    "include_hs_in_features": true,
    "drop_columns": [],
    "precision": "float32",
    "validate_sin_cos_circle": true,
    "circle_tolerance": 0.01
  },
  "splitting": {
    "angle_bins": 72,
    "hs_bins": 6,
    "test_size": 0.10,
    "val_size": 0.10,
    "seed": 123,
    "shuffle": true,
    "drop_incomplete_bins": false,
    "hs_binning_method": "quantile"
  },
  "models": {
    "native": ["ExtraTreesRegressor"],
    "wrapped": ["KNeighborsRegressor"]
  },
  "hyperparameters": {
    "enabled": true,
    "cv_strategy": "kfold_on_train",
    "cv_folds": 5,
    "grids": {
      "ExtraTreesRegressor": {
        "n_estimators": [100, 300, 600, 1000],
        "max_depth": [null, 20, 40, 60],
        "min_samples_split": [2, 5, 10],
        "min_samples_leaf": [1, 2, 4],
        "max_features": [1.0, "sqrt", "log2", 0.5],
        "criterion": ["squared_error", "friedman_mse"],
        "bootstrap": [false]
      },
      "KNeighborsRegressor": {
        "n_neighbors": [3, 5],
        "weights": ["uniform", "distance"]
      }
    }
  },
  "iterative": {
    "enabled": true,
    "max_rounds": 1,
    "min_features": 5
  },
  "execution": {
    "n_jobs": -1,
    "deterministic": true
  },
  "outputs": {
    "base_results_dir": "results",
    "save_predictions": true,
    "save_models": true
  },
  "logging": {
    "level": "INFO",
    "log_to_file": true,
    "log_to_console": true,
    "colorful_console": true
  }
}