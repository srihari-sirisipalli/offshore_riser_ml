{
  "data": {
    "file_path": "data/raw/Feature_Extraction_156_best_math_transformed_SignSafe.xlsx",
    "target_sin": "target_wave_direction_deg_sin",
    "target_cos": "target_wave_direction_deg_cos",
    "hs_column": "sea_elevation_significant_height_Hs_m",
    "include_hs_in_features": true,
    "drop_columns": [],
    "precision": "float32",
    "validate_sin_cos_circle": true,
    "circle_tolerance": 0.01
  },
  "splitting": {
    "angle_bins": 72,
    "hs_bins": 6,
    "test_size": 0.10,
    "val_size": 0.10,
    "seed": 123,
    "shuffle": true,
    "drop_incomplete_bins": false,
    "hs_binning_method": "quantile"
  },
  "models": {
    "native": [
      "ExtraTreesRegressor",
      "RandomForestRegressor",
      "MLPRegressor",
      "Ridge"
    ],
    "wrapped": [
      "HistGradientBoostingRegressor",
      "SVR"
    ]
  },
  "hyperparameters": {
    "enabled": true,
    "cv_strategy": "kfold_on_train",
    "cv_folds": 5,
    "grids": {
      "ExtraTreesRegressor": {
        "n_estimators": [100, 300, 500, 1000],
        "max_depth": [null, 20, 40, 60],
        "min_samples_split": [2, 5, 10],
        "min_samples_leaf": [1, 2, 4],
        "max_features": [1.0, "sqrt", "log2", 0.5],
        "bootstrap": [false, true]
      },
      "RandomForestRegressor": {
        "n_estimators": [100, 300, 500],
        "max_depth": [null, 20, 30, 50],
        "min_samples_split": [2, 5, 10],
        "min_samples_leaf": [1, 2, 4],
        "max_features": [1.0, "sqrt", 0.33]
      },
      "MLPRegressor": {
        "hidden_layer_sizes": [[100], [100, 100], [200, 100], [50, 50, 50], [256, 128, 64]],
        "activation": ["relu", "tanh"],
        "solver": ["adam", "sgd"],
        "alpha": [0.0001, 0.001, 0.01],
        "learning_rate": ["constant", "adaptive"],
        "learning_rate_init": [0.001, 0.01],
        "max_iter": [500]
      },
      "HistGradientBoostingRegressor": {
        "learning_rate": [0.01, 0.05, 0.1, 0.2],
        "max_iter": [100, 300, 500],
        "max_leaf_nodes": [31, 63, 127],
        "min_samples_leaf": [20, 50],
        "l2_regularization": [0.0, 0.1, 1.0]
      },
      "SVR": {
        "kernel": ["rbf", "poly", "sigmoid"],
        "C": [0.1, 1.0, 10.0, 100.0],
        "gamma": ["scale", "auto", 0.01, 0.1],
        "epsilon": [0.01, 0.1, 0.2]
      },
      "KNeighborsRegressor": {
        "n_neighbors": [3, 5, 7, 10, 15],
        "weights": ["uniform", "distance"],
        "p": [1, 2],
        "leaf_size": [30, 50]
      },
      "Ridge": {
        "alpha": [0.01, 0.1, 1.0, 10.0, 100.0],
        "solver": ["auto", "svd", "cholesky", "lsqr"]
      },
      "Lasso": {
        "alpha": [0.0001, 0.001, 0.01, 0.1, 1.0],
        "max_iter": [5000]
      },
      "ElasticNet": {
        "alpha": [0.001, 0.01, 0.1, 1.0],
        "l1_ratio": [0.1, 0.5, 0.9],
        "max_iter": [5000]
      },
      "KernelRidge": {
        "alpha": [0.1, 1.0, 10.0],
        "kernel": ["polynomial", "rbf", "laplacian"],
        "gamma": [null, 0.1, 1.0],
        "degree": [2, 3]
      },
      "GradientBoostingRegressor": {
        "n_estimators": [100, 300],
        "learning_rate": [0.01, 0.1],
        "max_depth": [3, 5, 7],
        "subsample": [0.8, 1.0],
        "max_features": ["sqrt", "log2", null]
      },
      "AdaBoostRegressor": {
        "n_estimators": [50, 100, 200],
        "learning_rate": [0.01, 0.1, 1.0],
        "loss": ["linear", "square", "exponential"]
      },
      "BaggingRegressor": {
        "n_estimators": [10, 50, 100],
        "max_samples": [0.5, 0.8, 1.0],
        "max_features": [0.5, 0.8, 1.0],
        "bootstrap": [true, false]
      },
      "SGDRegressor": {
        "loss": ["squared_error", "huber", "epsilon_insensitive"],
        "penalty": ["l2", "l1", "elasticnet"],
        "alpha": [0.0001, 0.001, 0.01],
        "learning_rate": ["invscaling", "adaptive"],
        "max_iter": [5000]
      }
    }
  },
  "iterative": {
    "enabled": false,
    "max_rounds": 10,
    "min_features": 5
  },
  "execution": {
    "n_jobs": -1,
    "deterministic": true
  },
  "outputs": {
    "base_results_dir": "results",
    "save_predictions": true,
    "save_models": true
  },
  "logging": {
    "level": "INFO",
    "log_to_file": true,
    "log_to_console": true,
    "colorful_console": true
  }
}