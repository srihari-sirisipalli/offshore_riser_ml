{
    "data": {
        "file_path": "Feature_Extraction_156_best_math_transformed_SignSafe.xlsx",
        "target_sin": "target_wave_direction_deg_sin",
        "target_cos": "target_wave_direction_deg_cos",
        "hs_column": "sea_elevation_significant_height_Hs_m",
        "include_hs_in_features": true,
        "drop_columns": [],
        "precision": "float32",
        "validate_sin_cos_circle": true,
        "circle_tolerance": 0.01
    },
    "data_quality_gates": {
        "enabled": true,
        "min_split_rows": 10,
        "max_missing_pct": 5.0,
        "min_feature_variance": 0.0,
        "max_constant_features": 0
    },
    "splitting": {
        "angle_bins": 12,
        "hs_bins": 6,
        "test_size": 0.1,
        "val_size": 0.1,
        "seed": 123,
        "shuffle": true,
        "drop_incomplete_bins": false,
        "hs_binning_method": "quantile",
        "cache_enabled": true,
        "cache_dir": ".cache/splits"
    },
    "models": {
        "native": [
            "ExtraTreesRegressor"
        ],
        "wrapped": []
    },
    "hyperparameters": {
        "enabled": true,
        "cv_strategy": "kfold_on_train",
        "cv_folds": 5,
        "max_trials": null,
        "parallel_configs": true,
        "resume_from_checkpoint": true,
        "grids": {
            "ExtraTreesRegressor": {
                "n_estimators": [
                    600,1000
                ],
                "max_depth": [
                    null, 40,60
                ],
                "min_samples_split": [
                    2,5
                ],
                "min_samples_leaf": [
                    1,2
                ],
                "max_features": [
                    "sqrt",0.5
                ],
                "bootstrap": [
                    false
                ],
                "criertion": [
                    "squared_error"
                ]
            }
        }
    },
    "iterative": {
        "enabled": true,
        "max_rounds": 2,
        "strategy": "lofo_test",
        "stopping_criteria": {
            "enabled": false,
            "patience_rounds": 5,
            "min_improvement_pct": 0.5,
            "metric": "cmae"
        },
        "lofo": {
            "parallel": true,
            "cv_folds": 5,
            "n_jobs": -1
        },
        "comparison": {
            "enabled": true,
            "metrics": [
                "cmae",
                "crmse",
                "mae",
                "rmse",
                "r2",
                "max_error"
            ]
        }
    },
    "execution": {
        "n_jobs": -1,
        "deterministic": true,
        "max_hours": null,
        "parallel_hpo_configs": true
    },
    "evaluation": {
        "bootstrap_samples": 0,
        "bootstrap_alpha": 0.05,
        "accuracy_bands": [
            0,
            5,
            10,
            20
        ],
        "compute_naive_baseline": true,
        "industry_baseline_cmae": null,
        "industry_baseline": {
            "enabled": false,
            "metrics_path": null,
            "predictions_path": null
        }
    },
    "error_analysis": {
        "enabled": true,
        "threshold_bins": [
            0,
            5,
            10
        ],
        "outlier_detection": "3sigma",
        "safety_gates": {
            "enabled": true,
            "max_critical_pct": 25.0,
            "max_warning_pct": 50.0,
            "critical_threshold_deg": 20.0,
            "warning_threshold_deg": 10.0
        }
    },
    "diagnostics": {
        "enabled": true,
        "parallel_plots": true,
        "plot_formats": [
            "png"
        ],
        "dpi": 150,
        "figsize": [
            10,
            6
        ]
    },
    "bootstrapping": {
        "enabled": true,
        "n_iterations": 100,
        "sample_ratio": 1.0,
        "confidence_level": 0.95,
        "metrics": [
            "cmae",
            "crmse",
            "accuracy_at_5deg",
            "max_error"
        ],
        "n_jobs": -1
    },
    "stability": {
        "enabled": false,
        "n_runs": 10,
        "seed_offset": 1000,
        "metrics": [
            "cmae",
            "crmse",
            "max_error"
        ],
        "parallel": true
    },
    "ensembling": {
        "enabled": false,
        "methods": [
            "mean",
            "median",
            "weighted"
        ],
        "weights": "inverse_error",
        "min_models": 3
    },
    "hpo_analysis": {
        "enabled": true,
        "optimal_top_percent": 10,
        "plot_types": [
            "param_importance",
            "convergence",
            "correlation_matrix"
        ],
        "save_optimal_ranges": true
    },
    "visualization": {
        "run_advanced_suite": true,
        "run_dashboard": true,
        "parallel_plots": true,
        "save_formats": [
            "png"
        ],
        "dpi": 150,
        "advanced_viz": {
            "error_surface_3d": true,
            "optimal_zone_map": true,
            "circular_error_plot": true,
            "boundary_analysis": true,
            "faceted_distributions": true,
            "performance_contours": true
        }
    },
    "outputs": {
        "base_results_dir": "results",
        "save_predictions": true,
        "save_models": true,
        "save_excel_copy": true,
        "save_reports_pdf": true,
        "compression": "snappy"
    },
    "logging": {
        "level": "INFO",
        "log_to_file": true,
        "log_to_console": true,
        "colorful_console": true,
        "log_file_name": "pipeline.log",
        "log_dir": "logs"
    },
    "resources": {
        "max_hpo_configs": 1000,
        "max_file_size_mb": 1000,
        "max_memory_mb": 8192,
        "operation_timeout_sec": 3600,
        "monitor_resources": true
    },
    "reproducibility": {
        "save_package": true,
        "include_environment": true,
        "include_data_checksums": true,
        "include_code_snapshot": false,
        "deployment_checklist": true
    },
    "reporting": {
        "enabled": true,
        "generate_executive_summary": true,
        "generate_technical_report": true,
        "include_plots": true,
        "formats": [
            "pdf",
            "html"
        ]
    },
    "data_integrity": {
        "enabled": true,
        "track_lineage": true,
        "validate_checksums": true,
        "fail_on_violation": false,
        "output_dir": "02_DataQualityChecks"
    },
    "global_tracking": {
        "enabled": true,
        "metrics_to_track": [
            "cmae",
            "crmse",
            "mae",
            "rmse",
            "r2",
            "max_error",
            "accuracy_at_5deg",
            "accuracy_at_10deg",
            "accuracy_at_20deg"
        ],
        "track_feature_evolution": true,
        "track_hyperparameters": true,
        "save_history": true
    }
}
